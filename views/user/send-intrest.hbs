<section>
    <div class="container mt-5 ml-5">
       {{#each profiles}}
<div class="card mb-3" style="max-width: 570px; max-height: 150px;">
  <div class="row g-0">
    <div class="col-md-4">
      <img style="width: 9rem; height: 9.3rem;" src="/profile-images/{{this._id}}1.jpg" class="img-thumbnail" alt="...">
    </div>
    <div class="col-md-8">
      <div class="card-body">
        <h5 class="card-title">{{this.Name}}</h5>
        <p class="card-text">{{this.age}} Years, {{this.religion}}, {{this.job}}, {{this.location}}</p>
        <a href="/view-profile/{{this._id}}" class="btn btn-secondary mr-4">View Profile</a>
        <a href="/cancel-intrest/{{this._id}}" class="btn btn-secondary">Cancel Intrest</a>
      </div>
    </div>
  </div>
</div>
 {{/each}}
